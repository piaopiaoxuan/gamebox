<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mcc.gamebox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:resizeableActivity="false">
        <activity
            android:name="com.example.mcc.gamebox.MainActivity"
            android:label="游戏"></activity>
        <activity
            android:name="com.example.mcc.gamebox.StartPage"
            android:theme="@style/NoActionBarFullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mcc.gamebox.Main2Activity"></activity>
    </application>

</manifest>